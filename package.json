	"scripts": {
		"prepare": "husky install",
		"start": "react-scripts start",
		"build": "react-scripts --max_old_space_size=4096 build",
		"test": "react-scripts test --watchAll=false",
		"test:coverage": "react-scripts test --silent --ci --coverage --watchAll=false --coverageReporters=\"text\" --coverageReporters=\"text-summary\" --json --outputFile=report.json",
		"eject": "react-scripts eject",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"lint": "eslint \"**/*.{js,jsx,json,ts,tsx}\" --no-error-on-unmatched-pattern --ignore-pattern src/setupTests.js --ignore-pattern node_modules/ --ignore-pattern build/ --max-warnings=0",
		"lint-fix": "yarn lint --quiet --fix",
		"lint-fix-warn": "yarn lint --fix",
		"ui": "yarn upgradeInteractive",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
