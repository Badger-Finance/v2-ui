{
  "name": "v3-ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.13.1"
  },
  "dependencies": {
    "@badger-dao/sdk": "^1.21.0",
    "@badger-dao/sdk-mocks": "^1.21.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@storybook/react": "^6.5.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/css-mediaquery": "^0.1.1",
    "@typescript-eslint/parser": "^5.29.0",
    "@typescript-eslint/typescript-estree": "^5.29.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "buffer": "^6.0.3",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "css-mediaquery": "^0.1.2",
    "d3-format": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ethers": "^5.6.9",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-fetch-mock": "^3.0.3",
    "mobx": "^6.6.0",
    "mobx-react-lite": "^3.4.0",
    "mobx-router": "^1.0.0",
    "os-browserify": "^0.3.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-number-format": "^4.9.3",
    "react-scripts": "5.0.1",
    "react-toastify": "9.0.3",
    "recharts": "^2.1.10",
    "slugify": "^1.6.5",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "web3modal": "^1.9.8"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "TZ=America/New_York react-app-rewired test --watchAll=false --runInBand --logHeapUsage",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "test:coverage": "TZ=America/New_York react-scripts test --silent --ci --coverage --watchAll=false --coverageReporters=\"text\" --coverageReporters=\"text-summary\" --json --outputFile=report.json",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "format": "yarn pretty-quick",
    "codegen:contracts": "typechain --target ethers-v5 --out-dir src/contracts src/abi/*.abi",
    "requirements-check": "node check-version.js",
    "postinstall": "yarn requirements-check",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js'",
    "link-crawler": "node check-urls.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "axios": "^1.2.4",
    "glob": "^8.0.3",
    "source-map-explorer": "^2.5.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!d3|d3-format|d3-array|internmap|delaunator|robust-predicates)"
    ],
    "moduleNameMapper": {
      "^d3-(.*)$": "d3-$1/dist/d3-$1"
    }
  }
}
